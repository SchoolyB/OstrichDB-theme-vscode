{
  "name": "ostrichdb-vscode-highlighting",
  "displayName": "ostrichdb-vscode-highlighting",
  "description": "Syntax highlighting for .ost files",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "MarshallBurns",
  "repository": {
    "url": "https://github.com/Solitude-Software-Solutions/OstrichDB-theme-vscode",
    "type": "git"
  },
  "contributes": {
    "languages": [{
      "id": "ost",
      "aliases": ["ostichdb", "ost"],
      "extensions": [".ost"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ost",
      "scopeName": "collections.ost",
      "path": "./syntaxes/ost.tmLanguage.json"
    }],
     "themes": [
      {
        "label": "OST Database Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/ost-color-theme.json"
      }
    ]
  }
}
