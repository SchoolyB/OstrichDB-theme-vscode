{
    "name": "OstrichDB Theme",
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "default"
    },
    "tokenColors": [
        {
            "scope": "keyword.control.ost",
            "settings": {
                "foreground": "#3f96de"
            }
        },
        {
            "scope": "string.quoted.double.ost",
            "settings": {
                "foreground": "#CE9178"
            }
        },
        {
            "scope": "constant.character.escape.ost",
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": "constant.numeric.integer.ost",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": "constant.numeric.float.ost",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": "constant.language.boolean.ost",
            "settings": {
                "foreground": "#b04577"
            }
        },
        {
            "scope": "constant.numeric.ost",
            "settings": {
                "foreground": "#B5CEA8"
            }
        },
        {
            "scope": "comment.line.hashtag.ost",
            "settings": {
                "foreground": "#6A9955"
            }
        }
    ]
}